---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>André Maia</title>
 <link href="http://andrenmaia.github.com/atom.xml" rel="self"/>
 <link href="http://andrenmaia.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://andrenmaia.github.com</id>
 <author>
   <name>André Maia</name>
   <email>andrenmaia@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://anmaia.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://andrenmaia.github.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
</feed>
